        "@types/express": "^4.17.13",
        "concurrently": "^7.2.2",
        "cross-env": "^7.0.3",
        "eslint": "^8.19.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-import-resolver-babel-module": "^5.3.1",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-react": "^7.30.1",
        "nodemon": "^2.0.19",
        "react-test-renderer": "^18.2.0"
